{
  "name": "innes",
  "version": "0.0.1",
  "type": "module",
  "main": "src/index.ts",
  "scripts": {
    "build": "bun run compile",
    "clean": "rm -rf dist",
    "dev": "bun run --watch src/index.ts",
    "lint": "eslint --format unix ./src  --max-warnings=0",
    "lint:fix": "eslint --format unix ./src  --max-warnings=0 --fix",
    "start": "bun src/index.ts",
    "typecheck": "tsc --noEmit",
    "compile": "bun run scripts/compile-token.ts"
  },
  "dependencies": {
    "@connectrpc/connect-node": "^2.1.0",
    "@towns-protocol/bot": "^0.0.398",
    "@towns-protocol/proto": "^0.0.398",
    "@towns-protocol/web3": "^0.0.398",
    "hono": "^4.7.11",
    "viem": "^2.38.6"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/bun": "^1.3.1",
    "@types/node": "^20.14.8",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import-x": "^4.10.2",
    "eslint-plugin-tsdoc": "^0.3.0",
    "hardhat": "^3.0.11",
    "typescript": "~5.8.3"
  },
  "files": [
    "/dist"
  ]
}